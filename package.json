{
  "name": "node-ts-template",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "engines": {
    "node": "^20"
  },
  "scripts": {
    "build": "npm run clean && swc src -d dist",
    "clean": "rm -rf dist",
    "check": "tsc --noEmit",
    "dev": "tsx watch --clear-screen=false ./src/main.ts | pino-pretty",
    "dev:debug": "tsx watch --clear-screen=false --inspect ./src/main.ts | pino-pretty",
    "fmt": "prettier --write .",
    "lint": "eslint src",
    "prepare": "husky install",
    "start": "node ./dist/main.js",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest"
  },
  "devDependencies": {
    "@swc/cli": "0.1.62",
    "@swc/core": "1.3.84",
    "@types/node": "20.6.0",
    "@typescript-eslint/eslint-plugin": "6.7.0",
    "@typescript-eslint/parser": "6.7.0",
    "@vitest/coverage-v8": "0.34.4",
    "eslint": "8.49.0",
    "husky": "8.0.3",
    "lint-staged": "14.0.1",
    "pino-pretty": "10.2.0",
    "prettier": "3.0.3",
    "tsx": "3.12.10",
    "typescript": "5.2.2",
    "undici": "5.24.0",
    "vitest": "0.34.4"
  },
  "dependencies": {
    "dotenv": "16.3.1",
    "pino": "8.15.1"
  }
}
